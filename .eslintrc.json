{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommend",
    "plugin: jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "node": {
        "extensions": [".jsx", "js"]
      }
    }
  },
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": false,
    "node": true
  }
}
